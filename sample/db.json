{
	"name": "default",
	"bucket": {
		"name": "my-bucket-name",
		"basePath": "giger/",
		"region" :"us-east-1"
	},
	"objects": {
		"avatar": {
			"bucketPath": "avatars/",
			"acl": "public-read",
			"maxAge": 86400,
			"constraints": {
				"minWidth": 720,
				"minHeight": 720,
				"ratio": "1/1"
			},
			"mimeTypes": [
				"image/jpeg"
			],
			"outputFormat" : "webp",
			"transforms": [
				{
					"name" : "L",
					"width" : 512,
					"quality" : 97
				},
				{
					"name" : "M",
					"width" : 256,
					"quality" : 90
				},
				{
					"name" : "S",
					"width" : 128,
					"quality" : 84
				},
				{
					"name" : "B",
					"width" : 128,
					"quality" : 21,
					"blur" : 4
				}
			]
		},
		"video": {
			"bucketPath": "videos/",
			"maxAge": 86400,
			"async": true,
			"mimeTypes": [
				"video/mp4"
			]
		}
	}
}
