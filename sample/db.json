{
	"name": "default",
	"bucket": {
		"name": "my-bucket-name",
		"basePath": "giger/", 
		"region" :"us-east-1" 
	},
	"objects": {
		"avatar": {
			"bucketPath": "avatars/",
			"maxAge": 86400,
			"constraints": {
				"minWidth": 720,
				"minHeight": 720,
				"ratio": "1/1"
			},
			"mimeTypes": [
				"image/jpeg"
			],
            "outputFormat" : "webp", 
			"transforms": [
				{
				    "name" : "L", 
					"width": 720,
					"quality": 97
				},
                {
				    "name" : "M", 
					"width": 480,
					"quality": 92
				}, 
				{
					"name" : "B", 
					"width": 100,
					"quality": 21,
					"blur": 4
				}
			] 
		},
		"video": {
			"bucketPath": "videos/",
			"maxAge": 86400,
			"async": true,
			"mimeTypes": [
				"video/mp4"
			]
		}
	}
}
